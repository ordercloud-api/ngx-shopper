{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "baseUrl": "../",
    "types": [],
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": false,
    "strictMetadataEmit": true,
    "fullTemplateTypeCheck": true
  },
  "exclude": [
    "test.ts", "**/*.spec.ts",
    "app/profile/",
    "app/checkout/",
    "app/order/"
  ]
}
